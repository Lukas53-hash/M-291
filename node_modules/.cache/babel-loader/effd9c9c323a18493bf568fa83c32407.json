nChanged","showAlert"],"sourceRoot":"06a_StateMgmt/01_Intro/src/components","sources":["SimpleState.vue"],"sourcesContent":["<template>\n\n  <div>\n    <b-alert v-model=\"showDismissibleAlert\" variant=\"danger\" dismissible>\n      Ausserdem bin ich {{ $store.state.age }}.\n    </b-alert>\n\n    <b-alert\n        :show=\"dismissCountDown\"\n        dismissible\n        variant=\"warning\"\n        @dismissed=\"dismissCountDown=0\"\n        @dismiss-count-down=\"countDownChanged\"\n    >\n      <p>Hallo, ich bin {{ $store.state.name }}. Diese Meldung verschwindet nach {{ dismissCountDown }} Sekunden...</p>\n      <b-progress\n          variant=\"warning\"\n          :max=\"dismissSecs\"\n          :value=\"dismissCountDown\"\n          height=\"4px\"\n      ></b-progress>\n    </b-alert>\n\n    <b-button @click=\"showAlert\" variant=\"info\" class=\"m-1\">\n      Mehr Informationen\n    </b-button>\n    <b-button @click=\"showDismissibleAlert=true\" variant=\"info\" class=\"m-1\">\n      Mehr Erfahren ({{ showDismissibleAlert ? 'sichtbar' : 'versteckt' }})\n    </b-button>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      dismissSecs: 7,\n      dismissCountDown: 0,\n      showDismissibleAlert: false\n    }\n  },\n  methods: {\n    countDownChanged(dismissCountDown) {\n      this.dismissCountDown = dismissCountDown\n    },\n    showAlert() {\n      this.dismissCountDown = this.dismissSecs\n    }\n  }\n}\n</script>\n"]}]}