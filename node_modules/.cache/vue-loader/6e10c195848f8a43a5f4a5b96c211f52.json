/div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      totalItems: 0,\n      totalCost: 0,\n      products: [\n        {title:\"Iphone 12 Pro Max\", currency: \"CHF\", prize: 850.00},\n        {title:\"Iphone 13 Pro Max\", currency: \"CHF\", prize: 1500.00},\n        {title:\"Iphone 11\", currency: \"CHF\", prize: 650.00},\n      ],\n    }\n  },\n  methods: {\n    addToCart(n) {\n      this.totalItems = this.totalItems + 1\n      this.totalCost = this.totalCost + n\n    },\n    formatPrize(currency, val) {\n      return `${currency} ${val.toFixed(2)}`\n    },\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.even-columns {\n  display: flex;\n  padding: 0.2em;\n}\n\n.even-columns > * {\n  flex: 1;\n}\n\n.card {\n  padding: 2em;\n  background: var(--clr-gradient);\n  color: #DA291C;\n  border-radius: 0.25rem;\n}\n\n*,\n*::before,\n*::after {\n  box-sizing: border-box;\n}\n\n:root {\n  --clr-primary: #DA291C;\n  --clr-secondary: #DA291C;\n  --clr-accent: #ffffff;\n  --clr-gradient: linear-gradient(\n          -90deg,\n          var(--clr-primary),\n          var(--clr-secondary)\n  );\n  --ff-title: bungee, sans-serif;\n  --ff-body: canada-type-gibson, sans-serif;\n  --fw-body: 300;\n  --fw-bold: 800;\n  --fw-title: 400;\n  --fw-number: 800;\n}\n\nbody {\n  padding: 2rem;\n  min-height: 80vh;\n  font-family: var(--ff-body);\n  font-weight: var(--fw-body);\n  font-size: 1.25rem;\n  display: flex;\n  flex-direction: column;\n}\n\na {\n  display: inline-block;\n  background: #DA291C;\n  border-radius: 10px;\n  font-size: 10px;\n  color: #ffffff;\n  padding: 5px 10px;\n  text-decoration: none;\n}\n.container {\n  width: min(95%, 30rem);\n  margin: 0 auto;\n}\n</style>\n"]}]}